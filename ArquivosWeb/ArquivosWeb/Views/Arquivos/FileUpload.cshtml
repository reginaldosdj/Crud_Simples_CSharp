
<form method="POST" enctype="multipart/form-data" asp-controller="Arquivos" asp-action="FileUpload">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload de arquivos</p>
            <input type="file" name="file" id="file" />
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Enviar arquivo" />
    </div>
    <div class="form-group">
        <span>@TempData["msg"]</span>
    </div>
</form>


